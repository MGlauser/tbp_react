  "version": "0.2.0",
  "configurations": [{
      "name": "Launch TBP via nodemon",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "nodemon",
      "program": "${workspaceRoot}/server/bin/tbp.js",
      "restart": true,
      "env": {
        "DEBUG": "tbp_react"
      },
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/start"
    }
  ]
}